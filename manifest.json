{
  "name": "Simplodorro",
  "version": "0.0.0.3",
  "manifest_version": 4,
  "description": "A very simple pomodoro timer.",
  "browser_action": {
    "default_icon": "icon_16.png"
  },
  "icons" : {
    "16"  : "icon_16.png",
    "48"  : "icon_48.png",
    "128" : "icon_128.png"
  },
  "web_accessible_resources" : [ "icon_16.png" ],
  "permissions": [ "notifications" ],
  "background": {
    "scripts" : ["background.js"]
  }
}
